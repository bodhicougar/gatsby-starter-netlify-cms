{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "comma-dangle": 0,
        "react/jsx-uses-vars": 1,
        "react/display-name": 1,
        "react/prop-types": "off",
        "no-unused-vars": "off",
        "no-console": "warn",
        "no-unexpected-multiline": "warn",
        "no-unsafe-finally": "off",
        "no-native-reassign": "off",
        "complexity": [
            "off",
            20
        ],
        "require-yield": "error",
        "no-process-env": "off",
        "no-undef": "off"
    }
}